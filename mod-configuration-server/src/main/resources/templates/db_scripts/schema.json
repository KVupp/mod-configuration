{
  "scripts": [
    {
      "run": "after",
      "snippet": "COPY config_data (jsonb) FROM 'data/locales.data' ENCODING 'UTF8';",
      "fromModuleVersion": 0
    }
  ],
  "tables": [
    {
      "tableName": "config_data",
      "withMetadata": true,
      "withAuditing": true,
      "generateId": true,
      "populateJsonWithId": true,
      "ginIndex": [
        {
          "fieldName": "value",
          "tOps": "ADD",
          "caseSensitive": false,
          "removeAccents": true
        } 
      ],
      "likeIndex": [
        {
          "fieldName": "configName",
          "tOps": "ADD",
          "caseSensitive": false,
          "whereClause": "WHERE ((jsonb->>'enabled'))::boolean = true",
          "removeAccents": false
        },
        {
          "fieldName": "code",
          "tOps": "ADD",
          "caseSensitive": false,
          "removeAccents": false
        }
      ],
      "uniqueIndex": [
        {
          "fieldName": "module, configName, code",
          "whereClause": "WHERE ((jsonb->'code')) is not null AND ((jsonb->'userId')) is null",
          "tOps": "ADD"
        },
        {
          "fieldName": "module, configName",
          "whereClause": "WHERE ((jsonb->'code')) is null AND ((jsonb->'userId')) is null",
          "tOps": "ADD"
        },
        {
          "fieldName": "module, configName, code, userId",
          "whereClause": "WHERE ((jsonb->'code')) is not null AND ((jsonb->'userId')) is not null",
          "tOps": "ADD"
        }
      ],
      "index": [
        {
          "fieldName": "module",
          "tOps": "ADD",
          "removeAccents": false
        },
        {
          "fieldName": "default",
          "tOps": "ADD",
          "stringType": false
        }    
      ]
    }
  ]
}
